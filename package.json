{
    "name": "@anon.3mpx/pulsechain-testnet-router",
    "description": "Router configured for PulseChain Testnet",
    "main": "dist/evm.js",
    "module": "dist/evm.mjs",
    "types": "dist/evm/index.d.ts",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "tsup --watch",
        "test": "vitest --run",
        "update:snapshot": "vitest -u",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@cryptoalgebra/custom-pools-sdk": "npm:@anon.3mpx/integral-sdk@1.0.0",
        "@pancakeswap/sdk": "npm:@anon.3mpx/tpls-sdk@1.0.1",
        "@pancakeswap/swap-sdk-core": "1.0.0",
        "@pancakeswap/token-lists": "0.0.9",
        "@pancakeswap/v3-sdk": "3.7.9",
        "@types/node": "^20.11.17",
        "async-retry": "^1.3.1",
        "debug": "^4.3.4",
        "graphql": "^16.8.1",
        "graphql-request": "5.0.0",
        "jsbi": "^3.2.5",
        "lodash": "^4.17.21",
        "mnemonist": "^0.38.3",
        "stats-lite": "^2.2.0",
        "tiny-invariant": "^1.3.0",
        "viem": "1.19.11",
        "zod": "^3.22.3"
    },
    "devDependencies": {
        "@types/async-retry": "^1.4.4",
        "@types/debug": "^4.1.7",
        "@types/lodash": "^4.14.178",
        "@types/stats-lite": "^2.2.0",
        "tsup": "^6.7.0",
        "typescript": "^5.4.5"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/evm/index.d.ts",
            "import": "./dist/evm.mjs",
            "require": "./dist/evm.js"
        },
        "./evm": {
            "types": "./dist/evm/index.d.ts",
            "import": "./dist/evm.mjs",
            "require": "./dist/evm.js"
        }
    },
    "version": "1.0.2"
}
